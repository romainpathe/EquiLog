<Application x:Class="EquiLog.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EquiLog"
             StartupUri="Windows/Login.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                
                <ResourceDictionary Source="Assets/Styles/DarkTheme.xaml"/>
                
                <ResourceDictionary Source="Assets/Icons.xaml"/>

                <ResourceDictionary Source="assets/Styles/MainStyle.xaml"/>
                
                <ResourceDictionary Source="Assets/Styles/ListViewStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
